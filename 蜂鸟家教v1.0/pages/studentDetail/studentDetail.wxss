page{
     height: 100%;
 }
.page{
    font-family: 'PingFang SC', 'Helvetica Neue', Helvetica, 'Droid Sans Fallback', 'Microsoft Yahei', sans-serif;
    height: 100%;
    background: #efefef;
}

.teacher-list{
  display:inline;
  padding-top: 40rpx;
}

.student-sketch{
  display: flex;
  background-color: #fff;
}

 .course-circular {
     display: flex;
     width: 140rpx;
     height: 140rpx;
  margin:30rpx 0rpx 30rpx 30rpx;
     border-radius:100rpx;
 }

 .course-circular text{
   color:#fff;
   font-size: 42rpx;
   margin: 45rpx 0 0 32rpx;
 }

 .math{
  background: #fa88fc;
}
.english{
  background:#87ff3d;
}
.chinese{
  background:#8061fa;
}
.physics{
  background:#ff7880;
}
.chemistry{
  background:#f5e347;
}
.biology{
  background:#56cbdb;
}
.politics{
  background:#f2b94b;
}
.history{
  background:#4bf298;
}
.geography{
  background:#49fc5b;
}
.art{
  background:#8a62cc;
}
.piano{
  background:#a0f238;
}
.Japanese{
  background:#c361ff;
}
.korean{
  background:#637cff;
}

.complaint{
    text-align: center;
    position:absolute ;
    top: 10rpx;
    right: 20rpx;
}

.complaintIcon{
  width: 50rpx;
  height: 70rpx;
}


.course-check{
   text-align:center;
    display:inline-block;
  margin: 0 auto;
}

.courseItem{
  margin:8rpx 8rpx 8rpx 0rpx;
  /* padding: 15rpx 25rpx 15rpx 25rpx; */
  
  float:left;
  border-radius: 40rpx;
  display:inline;
  color: #737373;
  font-size: 30rpx;
}



 .shareIcon{
  width: 50rpx;
  height: 50rpx;
} 

.share-left{
  float: left;
  width:25%;
  height:100%;

}


.teacher-name{
    font-size: 42rpx;
}

.name-salary{
    display: block;
    margin: 38rpx 0rpx 0rpx 20rpx;
}

.aboutData{
background-color: #fff;
  padding: 0rpx 0rpx 20rpx 30rpx;
  font-size: 30rpx;
  color: #737373;
  display: flex;
}

.timeIcon,.collectIcon,.applyIcon{
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
  border:none;
}

.timeText,.collectText{
  vertical-align:middle;
  margin-right: 55rpx;
}

.applyText{
  vertical-align:middle;
}


.student-info,.remark,.teacher-requirement,.telephone{
  background-color: #fff;
  margin-top: 15rpx;
  padding: 30rpx;
}

.address{
    background-color: #fff;
  margin-top: 15rpx;
  padding: 30rpx 10rpx 30rpx 30rpx;
    position: relative;
    align-items:center;
  display: flex;
}

.mapIcon{
  width: 80rpx;
  height: 90rpx;
}


.teach-salary{
  color: #fe4c40;
  margin-top: 40rpx;
}

.basic,.teacher-sex,.degree{
  display: flex;
  padding-top: 20rpx;
}


.gray-text{
  color: #737373;
}


.remark-title{
  display: flex;
  color:#fe4c40;
  margin-bottom: 20rpx;
  
}

.remark-int{
  width: 100%;
  display: flex;
  margin-right: 30rpx;
}


.tip{
  width: 100%;
  font-size: 30rpx;
  padding-top: 30rpx;
  padding-bottom: 150rpx;
  background:#efefef;
  text-align: center;
  color:#fe4c40;
}


.bottom{
  z-index: 99;
  position:fixed;
  bottom:0;
  text-align: center;
  width: 100%;
  height:100rpx;
  background: #fff;

}

.talk-right{
    width:50%;
    height:100%;
    display: flex;

}

.telephone-icon{
  height:60rpx;
  width: 60rpx;
  margin-left: 20rpx;
}

.collect-icon{
  height:60rpx;
  width: 60rpx;
  margin:auto;
}

.collect-left{
  width:25%;
  float: left;
  background-color:#fff;
}

.talk-right{
  width: 50%;
  float: right;
  background-color:#fe4c40;
  color:#fff;
}

.collect{
  width: 40%;
  height: 80%;
}

.talk-right text{
  margin: auto;
  display: flex;
}

.collect,.collect-text{
  margin: auto;
  font-size: 27rpx;
  color: #737373;
}

.logo{
  width: 120rpx;
  height: 120rpx;
  top: -60rpx;
  position: absolute;
  margin-left: -60rpx;
    z-index: 101;
}

 .goldUser{
   box-shadow:   0 2px 16px #ffea2e, 0 0 1px #ffea2e, 0 0 1px #ffea2e;  
  z-index: 100;
   position: absolute;
  top: 20%;
  left: 50%;
  margin-left: -260rpx;
  border-radius: 20rpx;

  background-color: #ffea2e;
  text-align: center;
    display: block;
    padding: 120rpx 80rpx 80rpx 80rpx;
} 

.openTitle{
  font-size: 50rpx;
  color: #595858;
  padding-bottom: 20rpx;
} 

.openContent{
  margin-top: 50rpx;
  font-size: 30rpx;
  color: #737273;
}

.openButton{
  margin: auto;
  margin-top: 70rpx;
  font-size: 40rpx;
  width: 300rpx;
  background-color: #fe4c40;
  line-height:2.2;
  border: 0;
}

 .openButton text{
  color: #fff;
}  

.giveUp{
  margin-top: 40rpx;
  color: #878585;
  font-size: 30rpx;
}

.detailText{
  margin-bottom: 20rpx;
}

.goldUserOpacity{
  display: none;
}


.fullbg{
    display:none;
    position: fixed;
    top:0;
    z-index: 1;
    width: 100%;
    height: 100%;
    background: rgb(1, 1, 1);
    transition: all 2s;
    
}
.fullopacity{
    display:flex;
    opacity: .5;
}